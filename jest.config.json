{
  "rootDir": ".",
  "testEnvironment": "jsdom",
  "setupFilesAfterEnv": [
    "<rootDir>/src/unit-test/setup.ts"
  ],
  "roots": ["<rootDir>/src/"],
  "moduleNameMapper": {
    "\\.(svg)$": "<rootDir>/src/unit-test/fileMock.ts"
  },
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx"],
  "transformIgnorePatterns": ["/node_modules/"],
  "testMatch": ["**/?(*.)test.ts?(x)"],
  "moduleDirectories": ["node_modules"]
}
